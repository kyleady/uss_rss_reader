<% if local_assigns[:full] %>
  <h1><%= feed.title %>
<% else %>
  <p><%= link_to feed.title, feed_path(feed) %>
<% end %>

<span style = "float:right;">
  <%= "(" %>
  <%= link_to "x", feed_path(feed), method: :delete %>
  <%= ")" %>
</span>

<% if local_assigns[:full] %>
  </h1>
  <p><%= feed.url %></p>
  <p><%= feed.description %></p>
  <%= render feed.articles %>
<% else %>
  </p>
<% end %>
